<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>AdminLTE WebUI - @ViewBag.Title</title>
    <link href="@Url.Content("~/Scripts/AdminLTE/bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/AdminLTE/source-sans-pro-woff2/source-sans-pro.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/AdminLTE/dist/css/AdminLTE.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/AdminLTE/font-awesome/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/AdminLTE/ionicons/css/ionicons.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/AdminLTE/dist/css/skins/_all-skins.min.css")" rel="stylesheet" type="text/css" />
    <!--[if lt IE 9]>
    <script src="@Url.Content("~/Scripts/AdminLTE/html5shiv/html5shiv.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/AdminLTE/respond/respond.min.js")" type="text/javascript"></script>
    <![endif]-->
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <a href="/Home/Index" class="logo">
              <!-- mini logo for sidebar mini 50x50 pixels -->
              <span class="logo-mini"><b>A</b>LT</span>
              <!-- logo for regular state and mobile devices -->
              <span class="logo-lg"><b>Admin</b>LTE</span>
        </a>
        @RenderBody()
    </div>

    <script src="@Url.Content("~/Scripts/AdminLTE/plugins/jQuery/jquery-2.2.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/AdminLTE/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
  
    <script src="@Url.Content("~/Scripts/AdminLTE/dist/js/app.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/AdminLTE/plugins/slimScroll/jquery.slimscroll.min.js")" type="text/javascript"></script>
    
</body>
</html>
